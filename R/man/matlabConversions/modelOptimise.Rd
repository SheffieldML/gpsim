\name{modelOptimise}
\Rdversion{1.0}
\alias{modelOptimise}
\title{Optimise the given model.}
\description{
  is a wrapper function that optimises a given model.
}
\usage{
model <- modelOptimise(model, P1, P2, P3...)
}
\arguments{
  \item{model}{the model to be optimised.}
  \item{P1, P2, P3...}{optional additional arguments.}
}
\value{
  \item{model}{the optimised model.}
}
\seealso{
\code{\link{modelObjective}, \link{modelGradient}}.
}
\examples{
## missing
}
\keyword{model}
