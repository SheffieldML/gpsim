\name{gpsimMapUpdateG}
\Rdversion{1.0}
\alias{gpsimMapUpdateG}
\title{Update the nonlinear transformation of f.}
\description{
  updates the fields of model associated with the non-linear transformation of f, namely g, g_grad and g_grad2.
}
\usage{
gpsimMapUpdateG(model, model')
}
\arguments{
  \item{model}{the model to be updated.}
  \item{model'}{the model with the updated g representation.}
}
\value{
}
\seealso{
\code{\link{gpsimMapFunctionalExpandParam}, \link{gpsimMapCreate}}.
}
\examples{
## missing
}
\keyword{model}
