@ARTICLE{Sandmann2007a,
  author = {Thomas Sandmann and Charles Girardot and Marc Brehme and Waraporn
	Tongprasit and Viktor Stolc and Eileen E M Furlong},
  title = {A core transcriptional network for early mesoderm development in
	{D}rosophila melanogaster.},
  journal = {Genes Dev},
