  journal = {IET Syst Biol},
  year = {2007},
  volume = {1},
  pages = {306--312},
  number = {5},
  month = {Sep},
  abstract = {Genes interact with each other in complex networks that enable the
	processing of information and the metabolism of nutrients inside
	the cell. A novel inference algorithm based on linear ordinary differential
	equations is proposed. The algorithm can infer the local network
	of gene-gene interactions surrounding a gene of interest from time-series
	gene expression profiles. The performance of the algorithm has been
	tested on in silico simulated gene expression data and on a nine
	gene subnetwork part of the DNA-damage response pathway (SOS pathway)
	in the bacteria Escherichia coli. This approach can infer regulatory
	interactions even when only a small number of measurements is available.},
  institution = {Telethon Institute of Genetics and Medicine, Via P. Castellino 111,
