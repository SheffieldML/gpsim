\name{translateKernParamInit}
\Rdversion{1.0}
\alias{translateKernParamInit}
\title{TRANSLATE kernel parameter initialisation.}
\description{
  initialises the input space translation kernel structure with some default parameters.
}
\usage{
kern <- translateKernParamInit(kern)
}
\arguments{
  \item{kern}{the kernel structure which requires initialisation.}
}
\value{
  \item{kern}{the kernel structure with the default parameters placed in.}
}
\seealso{
\code{\link{kernCreate}, \link{kernParamInit}}.
}
\examples{
## missing
}
\keyword{model}
