\name{simKernParamInit}
\Rdversion{1.0}
\alias{simKernParamInit}
\title{SIM kernel parameter initialisation.}
\description{
  initialises the single input motif kernel structure with some default parameters.
}
\usage{
kern <- simKernParamInit(kern)
}
\arguments{
  \item{kern}{the kernel structure which requires initialisation.}
}
\value{
  \item{kern}{the kernel structure with the default parameters placed in.}
}
\seealso{
\code{\link{kernCreate}, \link{kernParamInit}, \link{simKernCompute}}.
}
\examples{
## missing
}
\keyword{model}
