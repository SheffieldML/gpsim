\name{mlpKernParamInit}
\Rdversion{1.0}
\alias{mlpKernParamInit}
\title{MLP kernel parameter initialisation.}
\description{
  initialises the multi-layer perceptron kernel structure with some default parameters.
}
\usage{
kern <- mlpKernParamInit(kern)
}
\arguments{
  \item{kern}{the kernel structure which requires initialisation.}
}
\value{
  \item{kern}{the kernel structure with the default parameters placed in.}
}
\seealso{
\code{\link{kernCreate}, \link{kernParamInit}}.
}
\examples{
## missing
}
\keyword{model}
